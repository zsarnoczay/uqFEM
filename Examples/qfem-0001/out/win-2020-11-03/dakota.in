environment 
 tabular_data 
 tabular_data_file = 'dakotaTab.out' 

method,
 sampling
 sample_type = lhs 
 samples = 200 
 seed = 949

variables 
 active uncertain 
  normal_uncertain = 1
    means = 500 
    std_deviations = 100 
    descriptors = 'Au' 
  lognormal_uncertain = 1
    means = 250 
    std_deviations = 50 
    descriptors = 'Ao' 
  weibull_uncertain = 1
    alphas = 20 
    betas = 210 
    descriptors = 'E' 
  beta_uncertain = 1
    alphas = 2 
    betas = 2 
    lower_bounds = 20 
    upper_bounds = 30 
    descriptors = 'P' 



interface 
  analysis_driver = 'workflow_driver1.bat'
  fork
   parameters_file = 'paramsDakota.in'
   results_file = 'results.out' 
   aprepro 
   work_directory
     named 'workdir' 
     directory_tag
     directory_save
     copy_files = 'templatedir/*' 
  asynchronous evaluation_concurrency = 12

responses
 response_functions = 2
 response_descriptors = 'Node_3_Disp_2' 'Node_2_Disp_2' 
 no_gradients
 no_hessians

